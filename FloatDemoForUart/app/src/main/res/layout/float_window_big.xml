        android:layout_width="180dip"
        android:layout_height="40dip"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="12dip"
        android:text="J5_VGA_USB"
        />
    <Button 
        android:id="@+id/back"
        android:layout_width="180dip"
        android:layout_height="40dip"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="12dip"
        android:text="返回"
        />
